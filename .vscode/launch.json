{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Başlat",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cppp-implement/Hemex.app",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb için düzgün yazdırmayı etkinleştir",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Ayrıştırılmış Kod Varyantını Intel olarak ayarla",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        ""
      ],
      "stopAtEntry": false,
      "cwd": "/run/media/saqut/Depom/Çalışma Masası/HemexJS/cpp-implement",
      "environment": [],
      "program": "/run/media/saqut/Depom/Çalışma Masası/HemexJS/cpp-implement/build/Debug/outDebug",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}